class Solution:
    def getMinMax(self, arr):
        max_val = arr[0]
        min_val = arr[0]

        for i in range(1, len(arr)):
            if arr[i] > max_val:
                max_val = arr[i]
            if arr[i] < min_val:
                min_val = arr[i]

        return min_val, max_val 
